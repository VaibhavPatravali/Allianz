{
    "routes": [
        {
            "path": "login",
            "component": "loginComponent"
        },
        {
            "path": "dashboard",
            "component": "portalComponent",
            "children": [
                {
                    "path": "searchClaim",
                    "component": "searchclaimComponent"
                },
                {
                    "path": "claimDetails",
                    "component": "claimdetailsgridsComponent"
                }
            ]
        },
        {
            "path": "''",
            "redirectTo": "login",
            "pathMatch": "full",
            "default": true
        },
        {
            "path": "**",
            "component": "PageNotFoundComponent"
        }
    ]
}
